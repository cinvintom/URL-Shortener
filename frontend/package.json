{
  "name": "url-shortener-frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3000/",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "prestart": "node -e \"const fs=require('fs');if(!fs.existsSync('.env')&&fs.existsSync('.env.sample')){fs.copyFileSync('.env.sample','.env');}\"",
    "start": "react-scripts start",
    "prebuild": "node -e \"const fs=require('fs');if(!fs.existsSync('.env')&&fs.existsSync('.env.sample')){fs.copyFileSync('.env.sample','.env');}\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}